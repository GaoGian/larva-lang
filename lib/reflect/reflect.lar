//Type类，从任意值构建其类型对象
public native class Type
{
    public Type(Any a);

    public String str(); //返回类型的字符串表示
    public Any zero_value(); //返回此类型的zero_value
}

public native String type_str(Any a); //返回a的类型的字符串表示，快速版本，不需要new Type对象

//将larva基础类型值或对象封装为一个Value对象
public native class Value
{
    public Value(Any a);

    public Type type(); //返回此Value的Type
    public Any value(); //返回此Value维持的值
}
