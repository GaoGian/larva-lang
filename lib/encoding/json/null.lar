//json null对象
public class JsonNull
{
    JsonNull()
    {
        base = new JsonBase(this);
    }

    public String type()
    {
        return "null";
    }

    public char[] marshal()
    {
        return "null".to_char_array();
    }

    public bool is_null()
    {
        return true;
    }

    usemethod JsonBase base;
}

public final JsonNull NULL = new JsonNull();
