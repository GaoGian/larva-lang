//json bool对象
public class JsonBool
{
    JsonBool(bool v)
    {
        base = new JsonBase(this);
        this.v = v;
    }

    public String type()
    {
        return "bool";
    }

    public char[] marshal()
    {
        if (v)
        {
            return "true".to_char_array();
        }
        return "false".to_char_array();
    }

    JsonBase base usemethod;
    bool v;
}

public final JsonBool TRUE  = new JsonBool(true),
                      FALSE = new JsonBool(false);
