public class TypeError
{
    TypeError(Json json)
    {
        this.type = json.type();
    }

    public String str()
    {
        return type;
    }

    String type;
}

public class MarshalError
{
    usemethod GoError e;
}

public class UnmarshalError
{
    usemethod GoError e;
}

public class UnsupportedJsonType
{
    UnsupportedJsonType(String type)
    {
        this.type = type;
    }

    public String str()
    {
        return type;
    }

    String type;
}
