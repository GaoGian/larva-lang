//json str对象
public class JsonStr
{
    public JsonStr(String v)
    {
        base = new JsonBase(this);
        this.v = v;
    }

    public String type()
    {
        return "str";
    }

    public String marshal()
    {
        return marshal_str(v);
    }

    public String as_str()
    {
        return v;
    }

    usemethod JsonBase base;
    String v;
}
