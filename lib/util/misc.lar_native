package LARVA_NATIVE

import (
    "unsafe"
    "reflect"
)

func lar_func_@<<hash_float>>(n float32) int64 {
    if n == 0.0 || n == -0.0 {
        return 0
    }
    return int64(*(*uint32)(unsafe.Pointer(&n)))
}

func lar_func_@<<hash_double>>(n float64) int64 {
    if n == 0.0 || n == -0.0 {
        return 0
    }
    return *(*int64)(unsafe.Pointer(&n))
}
